					"collectionDate"
				],
				"properties": {
					"bin_type": {
						"$id": "#root/bins/items/bin_type", 
						"title": "Bin_type", 
						"type": "string",
						"default": "",
						"examples": [
							"Empty Standard Mixed Recycling"
						],
						"pattern": "^.*$"
					},
					"collectionDate": {
						"$id": "#root/bins/items/collectionDate", 
						"title": "Collectiondate", 
						"type": "string",
						"default": "",
						"examples": [
							"29/07/2022"
						],
						"pattern": "^.*$"
					}
				}
			}

		}
	}
